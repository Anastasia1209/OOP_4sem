        <Rectangle HorizontalAlignment="Left" Height="30" Margin="425,0,0,0" RadiusX="16" RadiusY="16" VerticalAlignment="Center" Width="130" Fill="#FFE67474"/>
        <Rectangle HorizontalAlignment="Left" Height="30" Margin="596,0,0,0" RadiusX="16" RadiusY="16" VerticalAlignment="Center" Width="130" Fill="#FFE67474"/>
        <Button x:Name="seeHotel" Content="посмотреть отели" HorizontalAlignment="Left" Margin="610,0,0,0" VerticalAlignment="Center" Foreground="White"  Background="#FFE67474" BorderThickness="0,0,0,0" Height="23" Width="102" Click="addHotel_Click" RenderTransformOrigin="0,0" Panel.ZIndex="10" />

    </Grid>

</Window>
